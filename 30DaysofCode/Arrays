// Day 7
// Given an array of integers, print elements in reverse order
// as a single line of space-separated numbers.
//
// @author Chinasa T. Okolo

import java.io.*;
import java.util.*;


public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        int[] arr = new int[n];
        // Read numbers in
        for(int i=0; i < n; i++){
            arr[i] = in.nextInt();
        }
        in.close();
        
        // Print in reverse
        for(int i=n-1; i >= 0; i--){
            System.out.print(arr[i]);
            System.out.print(" ");
        }
    }
}
